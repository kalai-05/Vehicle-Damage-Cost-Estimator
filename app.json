{
  "expo": {
    "name": "CRASH",
    "slug": "CRASH",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
     "backgroundColor": "#001f3d",
    "splash": {
      "image": "./assets/images/logo_with_bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#001f3d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "scheme": "CRASH",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_with_bg.png", 
        "backgroundColor": "#001f3d"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_with_bg.png",
      "backgroundColor": "#001f3d"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_with_bg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#001f3d"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ac0883a-8b1c-4746-b723-ea546eddaf13"
      }
    }
  }
}
